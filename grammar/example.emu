// Here is some example code written in Emu. This particular example is mainly used for fleshing out the grammar of the language.

func fact(n: 32): u32 {
    let acc: u32 = 1;
    for (let i: u32 = 2; i <= n; i++) {
        acc *= i;
    }
    return acc;
}

func matmul(comptime r: u32, comptime m: u32, comptime c: u32, const a: f32[r][m], const b: f32[m][c]): f32[r][c] {
    let i, j, k: u32;
    let out: f32[r][c] = 0;
    for (i = 0; i < r; i++) {
        for (j = 0; j < c; j++) {
            for (k = 0; k < m; k++) {
                out[i][j] += a[i][k] * b[k][j];
            }
        }
    }
    return out;
}
