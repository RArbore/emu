const size: i64 = 4;

func g(): f64[size] {
     c: bool = comptime size == 4;
     x: f64[size] = {0.0, 1.0, 2.0, 3.0};
     for (i: i64 = 0; i < size; ++i) x[i] *= 2.0;
     return x;
}

func f(): f64 {
     x: f64[size] = g();
     y: f64 = 0.0;
     for (i: i64 = 0; i < size; ++i) {
     	 y += x[i];
     }
     return y;
}
