const four: i64 = comptime 1 + 3;

func g(): f64[four] {
     return {1.0, 2.0, 3.0, 4.0};
}

struct s {
     x: i64, y: f64[four]
};

func h(): s {
     st: s = undefined;
     st.x = 1;
     st.y = {1.0, 2.0, 3.0, 4.0};
     return st;
}

func sum(x: f64[four]): f64 {
     return x[0] + x[1] + x[2] + x[3];
}

func f(): f64 {
     return sum(comptime g());
}
